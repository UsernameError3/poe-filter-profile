{
    "name": "poe-filter-profile",
    "version": "1.0.0",
    "description": "A profile manager for POE Filters.",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UsernameError3/poe-filter-profile.git"
    },
    "keywords": [
        "node",
        "template"
    ],
    "author": "Alex Fleming",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/UsernameError3/poe-filter-profile/issues"
    },
    "homepage": "https://github.com/UsernameError3/poe-filter-profile#readme",
    "scripts": {
        "build": "gulp build",
        "build-message": "echo \"Linting & Building Files...\" && exit 0",
        "commit-check": "gulp commit-status",
        "format": "npx eslint --fix \"./**/index.js\"",
        "test": "",
        "start": "",
        "stop": "",
        "coverage": "nyc npm run test"
    },
    "pre-commit": [
        "build-message",
        "format",
        "build"
    ],
    "nyc": {
        "check-coverage": true,
        "reporter": [
            "text",
            "text-summary",
            "html"
        ],
        "report-dir": "./coverage/",
        "exclude": [
            "**/*.spec.js"
        ],
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
    },
    "devDependencies": {
        "eslint": "^8.15.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.3.0",
        "nyc": "^15.1.0",
        "pre-commit": "^1.2.2"
    },
    "dependencies": {
        "dotenv": "^16.0.1"
    }
}
