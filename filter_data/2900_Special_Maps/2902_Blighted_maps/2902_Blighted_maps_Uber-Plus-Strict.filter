#------------------------------------
#   [2902] Blighted maps
#------------------------------------
# !! Override 530 : "blighted maps, enchanted maps, maps with special mods"

Show # $type->maps->blighted $tier->uber
	UberBlightedMap True
	Class "Maps"

Show # $type->maps->blighted $tier->t1
	BlightedMap True
	MapTier >= 13
	Class "Maps"

Hide # %H5 $type->maps->blighted $tier->any
	BlightedMap True
	Class "Maps"
